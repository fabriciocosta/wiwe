RewriteRule ^perfil/activation/+(.+)$ /principal/home/perfil.php?_mod_=activation&_activationcode_=$1 [L]
RewriteRule ^profile/activation/+(.+)$ /principal/home/perfil.php?_mod_=activation&_activationcode_=$1 [L]

RewriteRule ^perfil/+(.+)$ /principal/home/perfil.php?_mod_=$1 [L]
