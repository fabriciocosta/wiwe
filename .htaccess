RewriteRule ^obras/festivales/([^/]+)$ /principal/home/obras.php?_mod_=festivales&_valor_=$1 [L]
RewriteRule ^work/festival/([^/]+)$ /principal/home/obras.php?_mod_=festivales&_valor_=$1 [L]

RewriteRule ^repository/([^/]+)$ /repository/([^/]+) [L]

RewriteRule ^home$ /principal/home/home.php
RewriteRule ^Home$ /principal/home/home.php

#RewriteRule ^([^/]+)$ /principal/home/$1.php
#RewriteRule ^([^/]+)/+([0-9]+)$ /principal/home/$1.php?_cID_=$2

RewriteRule ^([^/]+)/+([0-9]+)$ /principal/home/$1.php?_cID_=$2 [L]
RewriteRule ^([^/]+)/+([^/]+)$ /principal/home/$1.php?_titulo_contenido_=$2 [L]
#RewriteRule ^([^/]+)/+([^/]+)$ /principal/home/$1.php?$2 [L]
RewriteRule ^([^/]+)$ /principal/home/$1.php [L]
